{
  "name": "appoloserver",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -Rf dist",
    "build": "babel --copy-files src --out-dir dist",
    "db:create": "npx sequelize db:create",
    "db:migrate": "npx sequelize db:migrate",
    "db:seed": "npx sequelize db:seed:all",
    "db:drop": "npx sequelize db:drop",
    "db:fresh": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "start": "nodemon ./src/index.js --exec babel-node",
    "serve": "node dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@apollo/subgraph": "^2.7.1",
    "@types/sequelize": "^4.28.20",
    "apollo-server": "^3.13.0",
    "dotenv": "^16.4.1",
    "graphql": "^16.8.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.0",
    "path": "^0.12.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.23.9",
    "@babel/node": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/node": "^20.11.7",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
